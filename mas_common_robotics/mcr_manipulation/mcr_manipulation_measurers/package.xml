<?xml version="1.0"?>
<package>
  <name>mcr_manipulation_measurers</name>
  <version>0.1.0</version>
  <description>
  A package to measure quantitative properties for manipulation tasks
  (e.g. measure the component-wise error between two poses).
  </description>
  <license>GPLv3</license>

  <maintainer email="jose.sanchez.loza@gmail.com">Jose Sanchez</maintainer>
  <author email="jose.sanchez.loza@gmail.com">Jose Sanchez</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rospy</build_depend>
  <build_depend>rostest</build_depend>

  <test_depend>unittest</test_depend>

  <run_depend>rospy</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>geometry_msgs</run_depend>
  <run_depend>mcr_manipulation_msgs</run_depend>
  <run_depend>tf</run_depend>
</package>